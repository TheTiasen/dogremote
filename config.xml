<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.dogremote" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Dog Remote</name>
    <description>Remote Control for Your Dog</description>
    <author email="mathias.alstadsater@gmail.com.com">Mathias Alstadsaeter</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="portrait" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="*" />
</widget>
